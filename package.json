{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "create-react-app": "^3.3.0",
    "dotenv": "^8.2.0",
    "font-awesome": "^4.7.0",
    "formik": "^2.1.7",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.0.0",
    "mdbreact": "^4.27.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.14",
    "react": "^16.12.0",
    "react-date-range": "^1.0.3",
    "react-datepicker": "^2.10.1",
    "react-datetime-picker": "^3.0.4",
    "react-dom": "^16.10.1",
    "react-loader": "^2.4.5",
    "react-moment": "^0.9.7",
    "react-notifications": "^1.4.3",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.1",
    "react-scripts": "3.1.2",
    "react-select": "^3.0.8",
    "react-slick": "^0.27.11",
    "react-widgets": "^4.4.11",
    "reactstrap": "^8.0.1",
    "recharts": "^2.0.0-beta.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1",
    "suneditor-react": "^2.14.4",
    "yarn": "^1.21.1",
    "yup": "^0.29.3"
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "import/imports-first": [
      "error",
      "absolute-first"
    ],
    "import/newline-after-import": "error",
    "import/prefer-default-export": 0,
    "semi": "error"
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true,
    "Headers": true,
    "Request": true,
    "fetch": true
  },
  "parser": "babel-eslint",
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 4,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
